        <div class="header-title">Mod of the week</div>
        <div class="card mod-of-the-week">
            <a href="/mod/<%= modOTW.id %>" class="title">SMB Modpack</a>
            <div class="authors">
                By
                <% for(let i=0; i < modOTW.authors.length; i++){ %>
                    <% let author = modOTW.authors[i] %>
                        <a href="/user/<%= author.id %>" class="author">
                            <% let str = author.username + (modOTW.authors.length-2 === i ? " & ": modOTW.authors.length-1 === i ? "":", "); %>
                                <%str.trim();%>
                                    <%= str %>
                        </a>
                        <% } %>
            </div>
            <a href="/mod/<%= modOTW.id %>" class="subtitle">The SMB modpack consists of over 5 different mods containing survival mode, wireless displays etc</a>
            <a href="/mod/<%= modOTW.id %>" class="images">
                <% for(let i=0; i < modOTW.images.length; i++){ %>
                    <% let image = modOTW.images[i] %>
                        <div class="image">
                            <img src="<%= image %>" />
                        </div>
                        <% } %>
            </a>
        </div>
        <div class="browse">
            <div class="sidebar">
                <div class="sidebar-bar">
                    <div class="title">Search</div>
                    <div class="search">
                        <input type="text" id="mod-search" placeholder="Search" />
                    </div>
                    <div class="verified search-option">
                        <div class="title">Verified</div>
                        <label class="switch">
						<input type="checkbox" checked />
						<span class="slider round"></span>
